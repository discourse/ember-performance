{
  "name": "ember-performance",
  "private": true,
  "version": "0.13.0",
  "description": "Benchmark Ember.js Performance",
  "repository": "https://github.com/NullVoxPopuli/ember-performance",
  "license": "MIT",
  "author": "Robin Ward",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build:prod": "pnpm _syncPnpm && ember build --environment=production",
    "build:dev": "pnpm _syncPnpm && ember build --environment=development",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint \"app/**/*\" ",
    "lint:hbs:fix": "ember-template-lint \"app/**/*\" --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "pnpm _syncPnpm && NODE_NO_WARNINGS=1 concurrently 'ember serve' 'pnpm _syncPnpm --watch' --names 'serve,inject'",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "_syncPnpm": "pnpm sync-dependencies-meta-injected"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@ember/optional-features": "^2.1.0",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.3.0",
    "@embroider/compat": "^3.6.0",
    "@embroider/core": "^3.4.14",
    "@embroider/webpack": "^4.0.4",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@glint/core": "1.4.0",
    "@glint/environment-ember-loose": "1.4.0",
    "@glint/environment-ember-template-imports": "1.4.0",
    "@glint/template": "1.4.0",
    "@nullvoxpopuli/eslint-configs": "^4.2.0",
    "@tsconfig/ember": "3.0.8",
    "@typescript-eslint/eslint-plugin": "8.0.0",
    "@typescript-eslint/parser": "8.0.0",
    "ascii-table": "^0.0.9",
    "bootstrap": "^4.6.2",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-funnel": "^3.0.8",
    "chart.js": "^4.4.3",
    "concurrently": "^8.2.2",
    "ember-auto-import": "^2.7.3",
    "ember-cli": "~5.10.0",
    "ember-cli-app-version": "^7.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-utils": "workspace:^",
    "ember-load-initializers": "^2.1.2",
    "ember-modifier": "^4.1.0",
    "ember-page-title": "^8.2.3",
    "ember-qunit": "^8.0.2",
    "ember-resolver": "^12.0.1",
    "ember-route-template": "^1.0.3",
    "ember-source": "~5.10.0",
    "ember-template-imports": "^4.0.0",
    "ember-template-lint": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.1.1",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qunit": "^8.1.1",
    "headjs": "^1.0.3",
    "loader.js": "^4.7.0",
    "pnpm-sync-dependencies-meta-injected": "^0.0.14",
    "prettier": "github:jurgenwerk/prettier#glimmer-embed-style-wip",
    "prettier-plugin-ember-template-tag": "2.0.2",
    "qunit": "^2.21.0",
    "qunit-dom": "^3.2.0",
    "tracked-built-ins": "^3.3.0",
    "typescript": "5.5.4",
    "webpack": "^5.91.0"
  },
  "engines": {
    "node": ">= 22.3.0"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "common": "workspace:^",
    "ember-3-28": "workspace:*",
    "ember-4-0": "workspace:*",
    "ember-5-10": "workspace:*",
    "ember-5-4": "workspace:*",
    "ember-5-5": "workspace:*",
    "ember-5-6": "workspace:*",
    "ember-5-7": "workspace:*",
    "ember-5-8": "workspace:*",
    "ember-5-9": "workspace:*",
    "ember-canary": "workspace:*",
    "ember-primitives": "^0.21.0",
    "qs-esm": "^7.0.2",
    "reactiveweb": "^1.3.0",
    "rsvp": "^4.8.5",
    "semver": "^7.6.3"
  },
  "volta": {
    "extends": "../package.json"
  },
  "dependenciesMeta": {
    "common": {
      "injected": true
    }
  }
}
