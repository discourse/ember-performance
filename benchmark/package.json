    "stylelint-config-standard": "^34.0.0",
    "tracked-built-ins": "^3.3.0",
    "typescript": "5.5.4",
    "webpack": "^5.91.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "common": "workspace:^",
    "perf-testing": "workspace:^",
    "rsvp": "^4.8.5"
  },
  "volta": {
    "extends": "../package.json"
  }
}
